<div id="artist-section" class="most-played-song-section">
  <h2 class= "favourite-header-text">Your most played artist is:</h2>

  <div class="most-played-artist-section-content">
    <ul class="most-played-song-ul-tag">
      <% for(let i = 0; i < topArtists.length; i++) { %>
        <li class="most-played-artist-li-tag"><%= topArtists[i].artist_name %></li>
      <% } %>
    </ul>
    <div class="most-played-artist">
      <h4><%= topArtistName %></h4>
      <img class="most-played-artist-images"
        src=<%= topArtistCover.url %>>
    </div>
  </div>
</div>