<div id="artist-section" class="most-played-x-container">

    <h2 class="favourite-header-text">Your most played artists are:</h2>

    <div class="most-played-x-section-content">

        <div class="most-played-artist-list">
            <ul class="most-played-artist-list-tag">
                <% for(let i = 0; i < topArtists.length; i++) { %>
                    <li class="most-played-artist-li-tag"><%= topArtists[i].artist_name %></li>
                <% } %>
            </ul>
        </div>

        <div class="most-played-artist-name-and-cover">
            <h1 class="most-played-artist-name"><%= topArtistName %></h1>
            <img id="cover" class="most-played-images-circle" src=<%= topArtistCover.url %>>
            <img id="blurred" class="most-played-images-circle" src=<%= topArtistCover.url %>>
        </div>

    </div>
</div>