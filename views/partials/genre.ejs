<div id="genre-section" class="most-played-x-container">
  <h2 class= "favourite-header-text">Your top genres <%= timeRangeContent %>: </h2>

  <div class="most-played-artist-section-content">
    <ul class="most-played-genre-ul-tag">
      <% for(let i = 0; i < topTenGenres.length; i++) { %>
        <li class="most-played-genre-li-tag"><%= topTenGenres[i][0] %></li>
      <% } %>
    </ul>
  </div>
</div>